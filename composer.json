{
  "name": "construct-blue/snappy-core",
  "require": {
    "php": "^8.2",
    "ext-json": "*",
    "construct-blue/snappy-renderer": "self.version",
    "league/route": "^5.1",
    "laminas/laminas-diactoros": "^3.0",
    "laminas/laminas-httphandlerrunner": "^2.6"
  },
  "autoload": {
    "psr-4": {
      "Blue\\Snappy\\Core\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "BlueTest\\Snappy\\Core\\": "tests"
    }
